kernel_addr_r=0x42000000
fdt_addr_r=0x48000000
fdtoverlay_addr_r=0x49000000
ramdisk_addr_r=0x48080000
scriptaddr=0x40000000
pxefile_addr_r=0x40100000
dfu_alt_info=sf 0:0=flash-bin raw 0x400 0x1f0000

serverip=192.168.3.51
ipaddr=192.168.4.57
dnsip=192.168.12.186
gatewayip=192.168.7.254
netmask=255.255.248.0

bootfile=kernel-consolinno-1u0025
fdtfile=oftree-consolinno-1u0025.dtb

bootcmd_usbnet=
	usb start;
	dhcp ${loadaddr} ${bootfile};
	dhcp ${fdt_addr_r} ${fdtfile};
	booti ${loadaddr} - ${fdt_addr_r}

bootcmd_emmc=
	mmc dev 2;
	ext4load mmc 2:2 ${loadaddr} /boot/${bootfile};
	ext4load mmc 2:2 ${fdt_addr_r} /boot/${fdtfile};
	booti ${loadaddr} - ${fdt_addr_r};

bootcmd=run bootcmd_emmc;
